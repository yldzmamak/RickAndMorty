{
  "name": "rickandmorty",
  "version": "1.0.0",
  "description": "Rick And Morty",
  "main": "root.tsx",
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn eslint --fix",
      "bash -c tsc --noEmit"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config environments/webpack/webpack.config.dev.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config environments/webpack/webpack.config.dev.js",
    "check": "npm run lint && npm run type-check",
    "clean": "sh ./scripts/clean.sh",
    "lint": "eslint . --cache && echo \"eslint: checking lint errors\"",
    "lint:fix": "eslint . --fix && echo \"eslint: no lint errors\"",
    "lint:style": "stylelint --fix \"**/*.less\" --custom-syntax postcss-less",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "start": "concurrently \"npm run json-server\" \"cross-env NODE_ENV=development webpack serve --mode development --config environments/webpack/webpack.config.dev.js\"",
    "json-server": "json-server --watch db.json --port 3000",
    "serve": "cross-env NODE_ENV=development webpack --config environments/webpack/webpack.config.dev.js && cross-env serve dist/",
    "type-check": "tsc",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "jest",
    "test:no-cache": "jest --no-cache --config ./jest.config.js",
    "test:watch": "jest --watch --config ./jest.config.js",
    "test:coverage": "jest --coverage",
    "version:major": "npm --no-git-tag-version version major",
    "version:minor": "npm --no-git-tag-version version minor",
    "version:patch": "npm --no-git-tag-version version patch"
  },
  "author": "yldzmamak",
  "license": "ISC",
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@reduxjs/toolkit": "^1.9.5",
    "antd": "^4.24.10",
    "axios": "^1.4.0",
    "i18next": "^22.4.15",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^12.3.1",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.12.1",
    "react-test-renderer": "^18.2.0",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@jest/globals": "^29.5.0",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-import": "^1.13.6",
    "babel-plugin-module-resolver": "^5.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "editorconfig": "^1.0.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsdoc": "^46.2.6",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "history": "^5.3.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "i18next-browser-languagedetector": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "image-minimizer-webpack-plugin": "^3.8.2",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.5.1",
    "jest-environment-jsdom": "^29.5.0",
    "jest-localstorage-mock": "^2.4.26",
    "jest-svg-transformer": "^1.0.0",
    "json-server": "^0.17.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.3",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^13.2.2",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.24",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.8",
    "react-svg": "^16.1.16",
    "style-loader": "^3.3.3",
    "stylelint": "^15.5.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-less": "^1.0.6",
    "stylelint-order": "^6.0.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.86.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
